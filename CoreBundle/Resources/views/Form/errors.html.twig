{% extends 'form_div_layout.html.twig' %}

{%- block form_errors -%}
    {%- if errors|length > 0 -%}
    <ul class="form-errors">
        {%- for error in errors -%}
        <li class="alert alert-danger">{{ error.message | trans }}</li>
        {%- endfor -%}
    </ul>
    {%- endif -%}
{%- endblock form_errors -%}
